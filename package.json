{
  "name": "anki-scripts",
  "scripts": {
    "lint": "tslint -c tslint.json -p .",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch true --sourceMap",
    "test": "node --trace-warnings node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit"
  },
  "author": "Emil Rehnberg <emil.rehnberg@gmail.com>",
  "version": "0.0.1",
  "description": "scripts for anki / ankidroid card viewer browser",
  "keywords": [
    "anki",
    "javascript",
    "typescript",
    "html-builders"
  ],
  "license": "MIT",
  "homepage": "https://github.com/emilrehnberg/anki-scripts",
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "globalSetup": "./jest.js",
    "watchman": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "json",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "test/.*\\.test\\.ts$",
    "coverageDirectory": "./coverage/"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "jest": "24.9.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.19.0",
    "typescript": "3.6.2"
  },
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}
